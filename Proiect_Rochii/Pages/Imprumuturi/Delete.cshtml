@page
@model Proiect_Rochii.Pages.Imprumuturi.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Imprumut</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Imprumut.Clienta)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Imprumut.Clienta.NumeClienta)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Imprumut.Rochie)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Imprumut.Rochie.Denumire)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Imprumut.DataImpumutare)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Imprumut.DataImpumutare)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Imprumut.DataReturnare)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Imprumut.DataReturnare)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="Imprumut.ID" />
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-page="./Index">Înapoi la listă</a>
    </form>
</div>
